{
	"cli": {
		"version": ">= 15.0.15",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"node": "20.18.0",
			"env": {
				"NPM_CONFIG_LEGACY_PEER_DEPS": "true"
			}
		},
		"development-simulator": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"NPM_CONFIG_LEGACY_PEER_DEPS": "true"
			}
		},
		"production": {
			"autoIncrement": true,
			"distribution": "store",
			"env": {
				"NPM_CONFIG_LEGACY_PEER_DEPS": "true"
			}
		}
	},
	"submit": {
		"production": {
			"ios": {
				"appleId": "illscience@gmail.com",
				"ascAppId": "6757837115"
			}
		}
	}
}
